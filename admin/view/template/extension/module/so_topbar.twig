                                <option value="social" {% if module_so_topbar_type and module_so_topbar_type == 'social' %}selected="selected"{% endif %}>{{ text_type_social_network }}</option>
                                <option value="banner" {% if module_so_topbar_type and module_so_topbar_type == 'banner' %}selected="selected"{% endif %}>{{ text_type_banner }}</option>
                                <option value="newsletter" {% if module_so_topbar_type and module_so_topbar_type == 'newsletter' %}selected="selected"{% endif %}>{{ text_type_newsletter }}</option>
