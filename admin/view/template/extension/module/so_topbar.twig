                                <option value="countdown2" {% if module_so_topbar_type and module_so_topbar_type == 'countdown2' %}selected="selected"{% endif %}>{{ text_type_countdown2 }}</option>
                                <option value="html" {% if module_so_topbar_type and module_so_topbar_type == 'html' %}selected="selected"{% endif %}>{{ text_type_html }}</option>
                            </select>
